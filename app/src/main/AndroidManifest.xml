<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.posttracking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:allowClearUserData="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.Dark"
        android:usesCleartextTraffic="true">
        <activity android:name=".UpdateInvoiceActivity"></activity>
        <activity android:name=".ViewQuotationActivity" />
        <activity android:name=".ViewInvoicesActivity" />
        <activity android:name=".ViewPackagesActivity" />
        <activity android:name=".CreateUpdatePackageActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".Z_ShowVehiclesTest" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity" />
    </application>

</manifest>